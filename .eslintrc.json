{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "eqeqeq": 2,
    "strict": [
      2,
      "global"
    ],
    "no-console": 1,
    "array-callback-return": 1,
    "block-scoped-var": 1,
    "consistent-return": 1,
    "curly": [
      1,
      "multi-line",
      "consistent"
    ],
    "no-empty-function": 1,
    "no-useless-return": 1,
    "vars-on-top": 1,
    "no-use-before-define": [
      0,
      "nofunc"
    ],
    "global-require": 1,
    "camelcase": 1,
    "new-cap": 1,
    "no-lonely-if": 1,
    "no-multiple-empty-lines": [
      1,
      {
        "max": 2
      }
    ],
    "no-trailing-spaces": 1
  }
}